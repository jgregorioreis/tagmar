                                </tr>
                            </table>
                    </div>
                    <div class="col-md-6"></div>
                </div>
                <div class="row dados">
                    {{#ifdf data.v_base 0}}
                    <div class="col-md-2">
                        <label><h4 class="mediaeval">Descrição: <a class='ativaDesc' title='Desativar edição do texto.'><i class="fas fa-toggle-on"></i></a></h4></label>
                    </div>
                    <div class="col-md-10">
                        <textarea class="mediaeval" name="data.descricao" style="height: 220px;font-size: 100%;" title="Escreve bastante ai." data-dtype="String">{{data.descricao}}</textarea>
                    </div>
                    {{/ifdf}}
