                                    <td><input name="data.moral" type="number" value="{{data.moral}}" /></td>
                                </tr>
                                <tr style="text-align: center;">
                                    <td><a class="rolarMoral" title="Rolar Moral"><i class="fas fa-dice-d20"></i></a></td>
                                </tr>
                            </table>
