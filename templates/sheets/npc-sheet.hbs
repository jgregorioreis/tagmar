<form class="{{cssClass}}" autocomplete="off" id="form_npc">
    <header class="sheet-header">
        <div class='container'>
            <div class="row container_dados">
                <div class="col-md-4 col-4">
                    <img class="tagmarLogo" src="systems/tagmar/templates/sheets/img/logo.png"/> 
                </div>
                <div class="col-md-8 col-8">
                    <h1 class="fairyDust" style="text-align: center; padding-top:20px;">{{actor.type}}</h1>
                </div>
            </div>
            <div class="row container_dados">
                <div class="col-md-2 col-4" style="padding-top: 3px;">
                    <img src="{{actor.img}}" data-edit="img" title="{{actor.name}}" height="64" width="64"/>
                </div>
                <div class="col-md-6 col-8">
                    <h1 class="fairyDust"><input name="name" type="text" value="{{actor.name}}" placeholder="Name"/></h1>
                </div>
                <div class="col-md-2 col-4" style="text-align: center;">
                    <label><h1 class="fairyDust" style="padding-top: 15px;">Estágio:</h1></label>
                </div>
                <div class="col-md-2 col-8">
                    <h1 class="fairyDust"><input class="ipEstagio" name="data.estagio" type="number" value="{{data.estagio}}" style="text-align: center";/></h1>
                </div>  
            </div>
        </div>
    </header>
    <section class="sheet-primary">
        <div class="container container_dados">
            <div class="row dados">
                <div class="col-md-12">
                    <nav class="prim-tabs tabs" data-group="primary">       <!--Tabs-->
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="basico" title="Basico">Básico</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="habilidades" title="Habilidades">Habilidades</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="combate" title="Combate">Combate</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="magia" title="Magia">Magia</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="pertences" title="Pertences">Pertences</a></label>
                    </nav>
                </div>
            </div>
            <div class="tab basico container" data-group="primary" data-tab="basico"> <!--Tab Básico-->
                <div class="row dados">         <!--Terceira Linha-->
                    <div class="col-md-2 col-4">
                        <label><h4 class="mediaeval">Altura(m):</h4></label>
                    </div>
                    <div class="col-md-2 col-8">
                        <input name="data.altura" type="text" value="{{data.altura}}" height="30" title="Altura do personagem, não a sua."/>
                    </div>
                    <div class="col-md-1 col-4">
                        <label><h4 class="mediaeval">Peso(Kg):</h4></label>
                    </div>
                    <div class="col-md-2 col-8">
                        <input name="data.peso" type="number" value="{{data.peso}}" height="30" title="Não adianta encolher a barriga."/>
                    </div>
                    <div class="col-md-5"></div>
                </div>
                <div class="row dados">  <!-- Table Atributos -->       <!--Quarta Linha-->
                    <div class="col-md-12">
                        <table>
                            <tr style="text-align: left;">
                                <th width="150px"></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="INT" title="Clique para rolar Teste.">INT</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="AUR" title="Clique para rolar Teste.">AUR</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="CAR" title="Clique para rolar Teste.">CAR</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="FOR" title="Já falei que isso vai dar merda.">FOR</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="FIS" title="Clique para rolar Teste.">FIS</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="AGI" title="Já entendeu né?">AGI</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="PER" title="Clique para rolar Teste.">PER</a></th>
                            </tr>
                            <tr>
                                <td class="mediaeval">Ajuste</td>
                                <td><input title="Atributo final do personagem." name="data.atributos.INT" type="number" value="{{data.atributos.INT}}"/></td>
                                <td><input title="Atributo final do personagem." class="ipAtrAUR" name="data.atributos.AUR" type="number" value="{{data.atributos.AUR}}"/></td>
                                <td><input title="Atributo final do personagem." name="data.atributos.CAR" type="number" value="{{data.atributos.CAR}}" /></td>
                                <td><input title="Atributo final do personagem." name="data.atributos.FOR" type="number" value="{{data.atributos.FOR}}" /></td>
                                <td><input title="Atributo final do personagem." class="ipAtrFIS" name="data.atributos.FIS" type="number" value="{{data.atributos.FIS}}" /></td>
                                <td><input title="Atributo final do personagem." name="data.atributos.AGI" type="number" value="{{data.atributos.AGI}}" /></td>
                                <td><input title="Atributo final do personagem." name="data.atributos.PER" type="number" value="{{data.atributos.PER}}" /></td>
                            </tr>
                            <tr>
                                <td class="mediaeval">Valor Para Teste</td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="data.valor_teste.INT" type="number" value="{{data.valor_teste.INT}}"/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="data.valor_teste.AUR" type="number" value="{{data.valor_teste.AUR}}"/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="data.valor_teste.CAR" type="number" value="{{data.valor_teste.CAR}}"/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="data.valor_teste.FOR" type="number" value="{{data.valor_teste.FOR}}"/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="data.valor_teste.FIS" type="number" value="{{data.valor_teste.FIS}}"/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="data.valor_teste.AGI" type="number" value="{{data.valor_teste.AGI}}"/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="data.valor_teste.PER" type="number" value="{{data.valor_teste.PER}}"/></td>
                            </tr>
                        </table>
                    </div>
                </div>
                <div class="row dados">     <!--Quinta Linha-->
                    <div class="col-md-12">
                        <table>
                            <caption class="mediaeval">Grupo de Armas</caption>
