>","total":{"valor":0,"valorKarma":0}},"flags":{"cf":{"id":"temp_yeylpn1ctq","path":"MAGIAS PERDIDAS#/CF_SEP/BARDOS#/CF_SEP/CONFRARIA DOS ARAUTOS PERDIDAS","color":"#000000"}},"img":"systems/tagmar/assets/0ASDASDASDASDASDASD6.png","effects":[],"_id":"xJMCUjhjaJyhIQKa"}
