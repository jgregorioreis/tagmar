i>","total":{"valor":0,"valorKarma":0}},"flags":{"cf":{"id":"temp_kq60oquyvz","path":"MAGIAS PERDIDAS#/CF_SEP/SACERDOTE#/CF_SEP/LENA PERDIDAS","color":"#000000"}},"img":"systems/tagmar/assets/0ASDASDASDASDASDASD6.png","effects":[],"_id":"QN90vZCkBcrAdGFx"}
